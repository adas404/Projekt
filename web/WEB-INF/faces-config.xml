<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="2.2"
              xmlns="http://xmlns.jcp.org/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">
    <managed-bean>
        <managed-bean-name>loginBean</managed-bean-name>
        <managed-bean-class>controller.LoginBean</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>carBean</managed-bean-name>
        <managed-bean-class>controller.CarBean</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>uzytkownikBean</managed-bean-name>
        <managed-bean-class>controller.UzytkownikBean</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>mapyBean</managed-bean-name>
        <managed-bean-class>controller.MapyBean</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <converter>
        <converter-id>uzytkownikConverter</converter-id>
        <converter-class>config.UzytkownikConverter</converter-class>
    </converter>
    <converter>
        <converter-id>carConverter</converter-id>
        <converter-class>config.CarConverter</converter-class>
    </converter>
 
</faces-config>
